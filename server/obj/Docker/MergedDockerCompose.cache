services:
  coreserver:
    build:
      context: C:\code\WssCoreServer\server
      dockerfile: CoreServer/Dockerfile
    expose:
    - '5000'
    image: coreserver
  nginx:
    build:
      context: C:\code\WssCoreServer\server
      dockerfile: ./nginx/Dockerfile
    links:
    - coreserver
    ports:
    - 80:80/tcp
    - 443:443/tcp
    - 5000:5000/tcp
version: '3.0'